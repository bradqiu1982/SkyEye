
@{
    ViewBag.Title = "OGPXYCompare";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<script type="text/javascript" src="~/Scripts/ImgTraining.js"></script>

<script type="text/javascript">
    $(function () {
        IMGTRAINING.OGPXYCMP();
    })
</script>

<style type="text/css">
    .CFD1 {
        background-color:#e5fbe5!important;    
    }
    .CFD3 {
        background-color:lightsalmon!important;
    }
</style>

<div class="row form-group">
    <div class="col-xs-4"></div>
    <div class="col-xs-4"><h1>OGP XY Recognize</h1></div>
    <div class="col-xs-4"></div>
</div>

<div class="row form-group">
    <div class="col-xs-2"><input class="form-control" type="text" value="Image Folder" readonly></div>
    <div class="col-xs-8"><input class="form-control" type="text" id="imgfolder" name="imgfolder"></div>
    <div class="col-xs-2"></div>
</div>

<div class="row form-group">
    <div class="col-xs-2"><input class="form-control" type="text" value="Wafer Number" readonly></div>
    <div class="col-xs-2"><input class="form-control" type="text" id="wafernum" name="wafernum"></div>
    <div class="col-xs-2">
        <select class="form-control" id="alg" name="alg">
            <option selected value="newalg">新算法</option>
            <option value="gen">经典算法</option>
            <option value="genangle">经典算法角度</option>
            <option value="newalgangle">新算法角度</option>
        </select>
    </div>
    <div class="col-xs-2">
        <select class="form-control" id="vtype" name="vtype">
            <option selected value="auto">自识别坐标类型</option>
            <option value="6inch">6英寸新坐标</option>
            <option value="4inch">4英寸经典</option>
            <option value="iivi">II-VI坐标</option>
        </select>
    </div>
    <div class="col-xs-1"><button class="btn btn-success" id="btn-recognize" name="btn-recognize">SPEED</button></div>
    <div class="col-xs-1"><button class="btn btn-success" id="btn-AI" name="btn-AI">AI 99</button></div>
    <div class="col-xs-1"><button class="btn btn-success" id="btn-review" name="btn-review">REVIEW</button></div>
</div>

<hr>

<div class="row form-group">
    <table class="table table-hover  table-condensed table-striped" id="imgtable" style="margin-top:1%">
        <caption class="tb-caption">OGP XY</caption>
        <thead id="imghead"></thead>
        <tbody id="imgcontent"></tbody>
    </table>
</div>



